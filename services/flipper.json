{
    "display_name": "Flipper",
    "path": "flipper.py",
    "description": "Flip all images in a website",
    "source": "https://github.com/AdrianVollmer/tlseraser",
    "install_cmd": "python3 -m pip install --user tlseraser",
    "argstring": "-p %(lport)s -l %(lhost)s -n %(netns)s",
    "parameters": {
        "lport": {
            "description": "LPORT",
            "type": "text",
            "help": "The local port to listen on",
            "value": "4321"
        },
        "lhost": {
            "description": "LHOST",
            "type": "text",
            "help": "The local IP address to listen on",
            "value": "0.0.0.0"
        },
        "netns": {
            "description": "netns",
            "type": "text",
            "help": "The name of the namespace to run in",
            "value": "default"
        }
    }
}
