{
    "display_name": "TLS Eraser",
    "path": "tlseraser",
    "description": "Terminate TLS connections and eavesdrop on the communication",
    "source": "https://github.com/AdrianVollmer/tlseraser",
    "install_cmd": "python3 -m pip install --user tlseraser",
    "argstring": "-p %(lport)s -l %(lhost)s",
    "parameters": {
        "lport": {
            "description": "LPORT",
            "type": "text",
            "help": "The local port to listen on",
            "value": "1234"
        },
        "lhost": {
            "description": "LHOST",
            "type": "text",
            "help": "The local IP address to listen on",
            "value": "0.0.0.0"
        }
    }
}
